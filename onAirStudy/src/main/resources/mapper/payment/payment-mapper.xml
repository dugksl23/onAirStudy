<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="payment">
 
	<update id='updatePayHistory'>
		update
			member
		set
			member_role = 'P' 
		where
			member_id = #{memberId}
	
	</update>
</mapper>